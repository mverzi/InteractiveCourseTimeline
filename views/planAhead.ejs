<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="/css/style.css">
   
    <title>Home</title>
</head>
<body>
    <h1>Plan Ahead</h1>
    <div class="navbar">
      <a href="/">Home</a>
      <div class="dropdown">
        <a class="dropbtn" href="/planAhead">Plan Ahead</a>
          <div class="dropdown-content">
            <a href="/discover">Discover</a>
            <a href="/planner">Planner</a>
            <a href="">Page 3</a>
          </div>
      </div>
      <a href="https://paws.gsu.edu" target="_blank">PAWS</a>
      <a href="#logout">Log Out</a>
  </div>
    
  <h1>My Plan</h1>
<ul>
  <li>
    Name: <%= student.name %><br>
    Credits: <%= student.creditsCompleted %><br>
    Courses Taken:
    <ul>
      <% student.coursesTaken.forEach(course => { %>
        <li><%= course.courseNumber %></li>
      <% }); %>
    </ul>
    Courses Not Taken:
    <ul>
      <% if (student.coursesNotTaken) { %>
        <% student.coursesNotTaken.forEach(course => { %>
          <li><%= course.courseNumber %></li>
        <% }); %>
      <% } else { %>
        <li>No courses not taken</li>
      <% } %>
    </ul>
  </li>
</ul>
    
      
    <!-- Your server.js script should be included separately -->
    <script src="server.js"></script>
</body>
</html>
